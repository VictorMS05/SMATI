{
    "name": "SMATI",
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/xbee/(.*)",
            "methods": [
                "GET",
                "POST"
            ],
            "dest": "/"
        },
        {
            "src": "/(.*)",
            "dest": "/web/$1"
        }
    ],
    "public": "web"
}