{
    "name": "SMATI",
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        },
        {
            "src": "./web/views/index.ejs",
            "use": "@vercel/static"
        },
        {
            "src": "./web/views/xbees/index.ejs",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/xbee/(.*)",
            "methods": [
                "GET",
                "POST"
            ],
            "dest": "/"
        },
        {
            "src": "/",
            "dest": "/web/views/index.ejs"
        },
        {
            "src": "/xbees",
            "dest": "/web/views/xbees/index.ejs"
        }
    ]
}